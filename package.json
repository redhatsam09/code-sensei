{
  "name": "code-sensei",
  "displayName": "Code Sensei",
  "description": "Your coding mentor that motivates and guides you through your programming journey.",
  "publisher": "your-name",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": ["Other"],
  "activationEvents": [
    "onView:codeSenseiView",
    "onCommand:codeSensei.openSensei"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "codeSensei",
          "title": "Code Sensei",
          "icon": "resources/forest-icon.svg"
        }
      ]
    },
    "views": {
      "codeSensei": [
        {
          "id": "codeSenseiView",
          "name": "Code Sensei",
          "type": "webview",
          "icon": "resources/forest-icon.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "codeSensei.openSensei",
        "title": "Code Sensei: Open Sensei",
        "icon": "$(tree-view-icon)"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "package": "echo 'Install vsce globally then run vsce package'",
    "lint": "echo 'No linter configured'"
  },
  "files": [
    "out",
    "assets",
    "resources",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/vscode": "^1.90.0",
    "typescript": "^5.4.0"
  }
}
